<!DOCTYPE html>
<html lang="en">
    <%- include('partials/head.ejs') %> 
    <%- include('partials/nav.ejs') %> 

    <body>
        <h1>New Video Upload</h1>
        <form method="POST" action="/streamDream">

            <div>
                <label for="artist">Artist Name:</label>
                <input id="artist" name="artist" type="text"/>
            </div>
            <div>
                <label for="img">Artist Photo:</label>
                <input id="img" name="img" type="text"/>
            </div>
            <div>
                <label for="headerImg">Header Image:</label>
                <input id="headerImg" name="headerImg" type="text"/>
            </div>
            <div>
                <label for="bio">Bio:</label>
                <input id="bio" name="bio" type="text"/>
            </div>
            <div>
                <label for="song">Song Name:</label>
                <input id="song" name="song" type="text"/>
            </div>
            <div>
                <label for="releaseDate">Release Date:</label>
                <input id="releaseDate" name="releaseDate" type="number"/>
            </div>
            <div>
                <label for="link">Video Link:</label>
                <input id="link" name="link" type="text"/>
            </div>
            <div>
                <label for="album">Album Name:</label>
                <input id="album" name="album" type="text"/>
            </div>
            <input type="submit" value="Upload Video">
        </form>



    </body>
</html>

